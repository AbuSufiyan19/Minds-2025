        <script>
           $(document).ready(function() {
            $('#collegeName').select2({
                placeholder: "Select College Name",
                closeOnSelect: false // Disable automatic close to manually handle it
            });

            // Show/Hide the "otherCollegeName" input based on the selected option
            $('#collegeName').on('change', function() {
                if (this.value === 'Others') {
                    $('#otherCollegeName').show();
                    $('#otherCollegeName').attr('required', 'required');
                } else {
                    $('#otherCollegeName').hide();
                    $('#otherCollegeName').removeAttr('required');
                }

                // Manually trigger the dropdown to close after an option is selected
                $('#collegeName').select2('close');
            });
        });

        </script>
        
