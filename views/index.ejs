<!-- ANIMATION JS FOR THE PAST sponsors SCROLL VIEW -->
<script>
  // Get all scroll elements
  const scrollElements = document.querySelectorAll('.scroll');

  // Add click event listeners
  scrollElements.forEach(scroll => {
    scroll.addEventListener('click', function (event) {
      // Check if the clicked element is a span or an img
      if (event.target.tagName === 'SPAN' || event.target.tagName === 'IMG') {
        // Toggle the paused class
        scroll.classList.toggle('paused');
      } else {
        // If clicked anywhere else in the scroll, continue animation
        scroll.classList.remove('paused');
      }
    });
  });
</script>


<script>
